{
  "name": "PortableBookmarks",
  "description": "Get bookmarks cross multiple browsers.",
  "version": "1.0",
  "manifest_version": 2,
  "author": "hsowan",
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+F",
        "mac": "MacCtrl+F"
      },
      "description": "Opens popup.html"
    }
  },
  "permissions": [
    "bookmarks"
  ]
}
